<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="disabledIcon.ico" />
    <None Remove="enabledIcon.ico" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="disabledIcon.ico" />
    <EmbeddedResource Include="enabledIcon.ico" />
  </ItemGroup>

</Project>